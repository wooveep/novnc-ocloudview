# 服务器配置
NODE_ENV=development
PORT=3000

# ocloudview API配置
OCLOUDVIEW_API_URL=http://192.168.40.161:8088
OCLOUDVIEW_API_KEY=your_api_key_here

# JWT配置
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=24h

# WebSocket配置
WEBSOCKET_PORT=6080
WEBSOCKET_PATH=/vnc

# VNC配置
VNC_PASSWORD_ENCRYPTION=true

# 数据库配置（如需要）
DB_HOST=localhost
DB_PORT=3306
DB_NAME=novnc_ocloudview
DB_USER=root
DB_PASSWORD=

# Redis配置（用于会话管理）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
