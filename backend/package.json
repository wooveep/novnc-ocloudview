{
  "name": "novnc-ocloudview-backend",
  "version": "1.0.0",
  "description": "Backend service for noVNC and ocloudview integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint src/",
    "docker:build": "docker build -t novnc-backend .",
    "docker:run": "docker run -p 3000:3000 -p 6080:6080 --env-file .env novnc-backend"
  },
  "keywords": [
    "novnc",
    "vnc",
    "ocloudview",
    "remote-desktop",
    "websocket"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.0",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "express-validator": "^7.0.1",
    "bcrypt": "^5.1.1",
    "@novnc/novnc": "^1.4.0",
    "winston": "^3.10.0",
    "express-rate-limit": "^6.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
